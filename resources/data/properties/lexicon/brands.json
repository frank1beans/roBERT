{
  "version": "1.1",
  "notes": "Marchi raggruppati per categoria verticale con sinonimi standardizzati per l'estrazione del campo \"marchio\".",
  "fallback": "Generico",
  "normalization": {
    "lowercase": true,
    "strip_accents": true,
    "remove_punctuation": true,
    "collapse_spaces": true,
    "trim": true,
    "split_spaced_letters": true,
    "remove_common_tokens": [
      "spa",
      "s.p.a.",
      "srl",
      "s.r.l.",
      "italia",
      "ceramica",
      "ceramiche",
      "rubinetteria",
      "rubinetterie",
      "vernici",
      "group",
      "parkett",
      "parquet",
      "porte",
      "infissi"
    ]
  },
  "matching": {
    "case_insensitive": true,
    "diacritics_insensitive": true,
    "mode": "token",
    "allow_substring": false,
    "min_token_length": 3,
    "word_boundary_regex": "\\b"
  },
