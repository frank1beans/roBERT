{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "version",
    "patterns",
    "normalizers"
  ],
  "properties": {
    "version": {
      "type": "string"
    },
    "patterns": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "property_id",
          "regex"
        ],
        "properties": {
          "property_id": {
            "type": "string"
          },
          "regex": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "normalizers": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      }
    },
    "normalizers": {
      "type": "object",
      "additionalProperties": {}
    }
  }
}