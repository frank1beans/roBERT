  "type": "object",
  "required": [
    "version",
    "computed"
  ],
  "properties": {
    "version": {
      "type": "string"
    },
    "computed": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "target",
          "expr"
        ],
        "properties": {
          "target": {
            "type": "string"
          },
          "expr": {
            "type": "string"
          }
        }
      }
    }
  }
}