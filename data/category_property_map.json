  "category_overrides": {
    "Elementi in laterizio": {
      "required": [
        "mat.materiale",
        "geo.spessore_elemento",
        "geo.foratura_laterizio",
        "mech.resistenza_compressione",
        "frs.resistenza_fuoco"
      ],
      "recommended": [
        "thm.lambda",
        "aco.rw",
        "geo.peso_areico",
        "mat.classe_reazione_fuoco"
      ],
      "ifc_classes": [
        "IfcWall",
        "IfcBuildingElementPart"
      ],
      "rules": [
        {
          "if": "contains(text, ['laterizio','forato','pieno','porizzato'])",
          "then": "boost_category('Elementi in laterizio', 1.0)"
        },
        {
          "if": "geo.foratura_laterizio in ['pieno','forato','semipieno']",
          "then": "require_group('geo')"
        }
      ]
    },
